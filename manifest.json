
{
  "manifest_version": 3,
  "name": "Beam Analyzer Professional",
  "version": "1.0.0",
  "description": "Professional structural beam analysis tool using the Matrix Stiffness Method.",
  "permissions": [
    "storage"
  ],
  "action": {
    "default_popup": "index.html",
    "default_title": "Beam Analyzer"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
